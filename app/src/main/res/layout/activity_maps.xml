<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".activity.MapsActivity">
    <include layout="@layout/toolbar" android:id="@+id/toolbar"/>


    <fragment
        android:id="@+id/map"
        android:layout_below="@id/toolbar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        class="com.google.android.gms.maps.SupportMapFragment"
        tools:context="****"
        />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="105dp"
        android:clickable="true"
        android:contentDescription="@string/ambil_layanan"
        android:focusableInTouchMode="true"
        android:src="@drawable/stethoscope_48px"
        android:tint="@color/common_google_signin_btn_tint"
        app:backgroundTint="#F44336"
        app:fabSize="normal"
        app:rippleColor="#00867C7C"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="20dp"
        android:focusable="true" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="25dp"
        android:src="@drawable/filled_chat_24px"
        app:backgroundTint="#FF9800"
        app:fabSize="normal"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="20dp" />
    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginRight="18dp"
        android:layout_marginBottom="187dp"
        app:srcCompat="@drawable/ic_arrow_back_white_24dp"
        app:backgroundTint="@color/colorAccentLight"
        app:fabSize="normal"
        app:rippleColor="#00867C7C"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="18dp"
        android:visibility="gone"/>

</RelativeLayout>